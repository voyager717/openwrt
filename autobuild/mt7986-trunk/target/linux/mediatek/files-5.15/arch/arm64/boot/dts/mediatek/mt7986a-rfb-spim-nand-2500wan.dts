/dts-v1/;
/plugin/;
#include "mt7986a-rfb-spim-nand.dts"

/ {
	compatible = "mediatek,mt7986a-2500wan-spim-snand-rfb";

        fragment@2 {
		target = <&mdio>;
                __overlay__ {
			phy5: phy@5 {
				compatible = "ethernet-phy-id67c9.de0a";
				reg = <5>;
				reset-gpios = <&pio 6 1>;
				reset-deassert-us = <20000>;
				phy-mode = "2500base-x";
			};
		
			phy6: phy@6 {
				compatible = "ethernet-phy-id67c9.de0a";
				reg = <6>;
				phy-mode = "2500base-x";
			};
		};
        };

        fragment@3 {
		target = <&switch>;
                __overlay__ {
			ports {
				port@4 {
					reg = <4>;
					label = "lan4";
				};
				port@5 {
					reg = <5>;
					label = "lan5";
					phy-mode = "2500base-x";

					fixed-link {
						speed = <2500>;
						full-duplex;
						pause;
					};
				};
			};
		};
        };
};
