			"0:wan" "1:lan:4" "2:lan:3" "3:lan:2" "4:lan:1" "5@eth0"
		;;
	luxul,xap-1610-v1)
		ucidef_add_switch "switch0" \
			"0:lan" "1:lan" "5@eth0"
		ucidef_set_interface_lan "eth0.1" "dhcp"
		;;
	luxul,xwr-3150-v1)
